{
  "name": "workplace",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve -o --poll 2000 --proxy-config proxy_local.conf.json",
    "start_aot": "node --max_old_space_size=8192 node_modules/@angular/cli/bin/ng serve --prod -o --poll 2000 --proxy=proxy_local.conf.js",
    "start_public": "ng serve -o --poll 2000 --host 0.0.0.0 --proxy=proxy_local.conf.js --disable-host-check",
    "build": "node --max_old_space_size=16384 node_modules/@angular/cli/bin/ng build --prod --base-href='/' --deploy-url='/static/' && sw-precache --root=dist --config=precache-config.js",
    "test": "ng test --watch=false",
    "test-ci": "ng test --no-watch --no-progress --browsers=ChromeHeadlessNoSandbox",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^8.2.14",
    "@angular/cdk": "^8.2.3",
    "@angular/common": "~8.1.0",
    "@angular/compiler": "~8.1.0",
    "@angular/core": "~8.1.0",
    "@angular/flex-layout": "^8.0.0-beta.27",
    "@angular/forms": "~8.1.0",
    "@angular/material": "^8.2.3",
    "@angular/platform-browser": "~8.1.0",
    "@angular/platform-browser-dynamic": "~8.1.0",
    "@angular/pwa": "^0.803.24",
    "@angular/router": "~8.1.0",
    "@angular/service-worker": "~8.1.0",
    "@types/hammerjs": "^2.0.36",
    "@types/mousetrap": "^1.6.3",
    "class-transformer": "^0.2.3",
    "date-fns": "^2.9.0",
    "hammerjs": "^2.0.8",
    "material-design-icons": "^3.0.1",
    "material-icons": "^0.3.1",
    "ngx-image-cropper": "^2.1.2",
    "reflect-metadata": "^0.1.13",
    "roboto-fontface": "^0.10.0",
    "rxjs": "~6.4.0",
    "sw-precache-webpack-plugin": "^0.11.5",
    "tslib": "^1.9.0",
    "vanilla-text-mask": "^5.1.1",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.801.0",
    "@angular/cli": "~8.1.0",
    "@angular/compiler-cli": "~8.1.0",
    "@angular/language-service": "~8.1.0",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "^5.2.1",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.5.1",
    "protractor": "^5.4.3",
    "ts-node": "~7.0.0",
    "tslint": "~5.15.0",
    "typescript": "~3.4.3"
  }
}
